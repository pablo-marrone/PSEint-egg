Algoritmo medidas

	Definir distanciaM, distanciaCM, distanciaP Como Real
	
	Escribir "ingrese la medida en metros"
	leer distanciaM
	
	distanciaCM=distanciaM*100 
	distanciaP=distanciaCM/2.54
	
	Escribir "la medida en centimetros es ", distanciaCM
	Escribir "la medida en pulgadas es ", distanciaP
	
FinAlgoritmo
